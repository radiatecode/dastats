{
    "name": "radiatecode/dastats",
    "description": "Record statistics or metrics to generate kpi / dashboard widget",
    "homepage": "https://github.com/radiatecode/dastats",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "radiatecode",
            "email": "radiate126@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RadiateCode\\DaStats\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RadiateCode\\DaStats\\tests\\" : "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers" : [
                "RadiateCode\\DaStats\\StatsServiceProvider"
            ]
        }
    }
}
